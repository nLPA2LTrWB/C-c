local drops = serv:Channel("trollüòà")
drops:Button("spawn nhi·ªÅu mob(boss sun orb)", function()
local mobTemplate = workspace["NPC DAMAGE"].Pride -- Mob g·ªëc
local spawnFolder = workspace["NPC DAMAGE"] -- Th∆∞ m·ª•c ch·ª©a mob
local spawnCount = 10 -- S·ªë l∆∞·ª£ng mob mu·ªën spawn
local spawnRadius = 20 -- B√°n k√≠nh spawn quanh v·ªã tr√≠ g·ªëc

for i = 1, spawnCount do
    local newMob = mobTemplate:Clone() -- T·∫°o b·∫£n sao mob
    newMob.Parent = spawnFolder -- ƒê∆∞a mob m·ªõi v√†o th∆∞ m·ª•c ch·ª©a mob
    newMob.Name = "Pride_" .. i -- ƒê·∫∑t t√™n mob ƒë·ªÉ ph√¢n bi·ªát
    
    -- ƒê·∫∑t v·ªã tr√≠ ng·∫´u nhi√™n xung quanh mob g·ªëc
    local randomX = math.random(-spawnRadius, spawnRadius)
    local randomZ = math.random(-spawnRadius, spawnRadius)
    newMob:SetPrimaryPartCFrame(mobTemplate.PrimaryPart.CFrame * CFrame.new(randomX, 0, randomZ))
    
    print("Spawned mob:", newMob.Name)
end
end)

drops:Button("dupe item(sun orb)", function()
local toolName = "Sun-Orb"
local toolStorage = game:GetService("ReplicatedStorage")

game.ReplicatedStorage.DupeTool.OnServerEvent:Connect(function(player)
    local tool = player.Backpack:FindFirstChild(toolName)
    if tool then
        local clonedTool = tool:Clone()
        clonedTool.Parent = player.Backpack
        print("Tool ƒë√£ ƒë∆∞·ª£c nh√¢n ƒë√¥i cho ng∆∞·ªùi ch∆°i:", player.Name)
    else
        warn("Kh√¥ng t√¨m th·∫•y tool:", toolName, "trong Backpack c·ªßa ng∆∞·ªùi ch∆°i:", player.Name)
    end
end)
end)

